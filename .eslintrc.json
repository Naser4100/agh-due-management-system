{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-console":"off",
        "linebreak-style":"off",
        "no-underscore-dangle":"off",
        "func-names":"off",
        "consistent-return":"off"
    }
}
